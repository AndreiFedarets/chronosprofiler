<adenium:View x:Class="Chronos.Client.Win.Views.Common.EventsTree.TimelineView"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:controls="clr-namespace:Chronos.Client.Win.Controls.Common.EventsTree"
            xmlns:adenium="clr-namespace:Adenium;assembly=Adenium">
    <Grid>
        <Border BorderBrush="{x:Static adenium:FontsAndColors.DarkNeutralBrush}" BorderThickness="1">
            <controls:Timeline Events="{Binding Events}" EventMessageBuilder="{Binding EventMessageBuilder}" ProfilingTimer="{Binding ProfilingTimer}" OpenCommand="{Binding OpenEventTreeCommand}" />
        </Border>
    </Grid>
</adenium:View>