<adenium:View x:Class="Chronos.Client.Win.Common.WebApplication.Views.ProfilingTargetSettingsView"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:adenium="clr-namespace:Layex.Views;assembly=Layex"
            HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
    <Grid>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <TextBlock Grid.Column="0" Text="Application Pool:" HorizontalAlignment="Right" />
            <ComboBox Grid.Column="1" ItemsSource="{Binding ApplicationPools}" SelectedItem="{Binding SelectedApplicationPool}" />
        </Grid>
    </Grid>
</adenium:View>
