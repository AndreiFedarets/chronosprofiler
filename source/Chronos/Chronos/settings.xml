<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <Logging LogLevel="Error">
    <FileLogger IsEnabled="True" LogLevel="Error"> <!-- All, Off, Critical, Error, Warning, Information, Verbose, ActivityTracing  -->
      <LogsDirectory Path="%ProgramData%\ChronosProfiler\Logs\" />
    </FileLogger>
    <EventLogger IsEnabled="False" LogLevel="Off" />
  </Logging>
  <CrashDump IsEnabled="True">
    <DumpsDirectory Path="%ProgramData%\ChronosProfiler\Dumps\" />
  </CrashDump>
  <Extensions>
    <Extension IsEnabled="true" Path="..\Chronos.Common\" />
    <Extension IsEnabled="true" Path="..\Chronos.Common.EventsTree\" />
    <Extension IsEnabled="true" Path="..\Chronos.Common.StandaloneApplication\" />
    
    <Extension IsEnabled="true" Path="..\Chronos.DotNet\" />
    <Extension IsEnabled="true" Path="..\Chronos.DotNet.BasicProfiler\" />
    <Extension IsEnabled="true" Path="..\Chronos.DotNet.ExceptionMonitor\" />
    <Extension IsEnabled="true" Path="..\Chronos.DotNet.SqlProfiler\" />
    <Extension IsEnabled="true" Path="..\Chronos.DotNet.TracingProfiler\" />

    <Extension IsEnabled="false" Path="..\Chronos.Java\" />
    <Extension IsEnabled="false" Path="..\Chronos.Java.BasicProfiler\" />

    <Extension IsEnabled="true" Path="..\Chronos.Client.Win\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.Common\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.Common.EventsTree\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.Common.StandaloneApplication\" />
    
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet.BasicProfiler\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet.ExceptionMonitor\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet.SqlProfiler\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet.TracingProfiler\" />
    <Extension IsEnabled="true" Path="..\Chronos.Client.Win.DotNet.FindReference\" />

    <Extension IsEnabled="false" Path="..\Chronos.Client.Win.Java\" />
    <Extension IsEnabled="false" Path="..\Chronos.Client.Win.Java.BasicProfiler\" />
  </Extensions>
  <ProfilingResults Path="%ProgramData%\ChronosProfiler\ProfilingResults\" />
  <HostConnections RunLocal="true">
    <!--<Connection MachineAddress="localhost" ObjectUid="{1B6E1937-B1A8-4E54-B2E0-D4DC1CD7642A}">
      <IpcChannel />
    </Connection>-->
  </HostConnections>
</Settings>